
{%extends "baseMain.html"%}
{%block body%}
<h3>Task Report (by ProcessCode/Sequence)</h3>
<a href='/updTask?cmd=ADD'>Add new task</a> <br/><br/>
<table border=1>
<tr>
  <th>Row</th>
  <th>TaskCode</th>
  <th>TaskDescription</th>
  <th>ProcessCode</th>
  <th>Sequence</th>
  <th>Is<br/>Manual</th>
  <th>DateTimeCreated</th>
  <th>DateTimeLastModified</th>
  <th>UserEmailCreated</th>
  <th>UserEmailLastModified</th>
</tr>
{%for task in taskList%} 
<tr>
<td><a href='/updTask?key={{task.key}}'>&nbsp;{{forloop.counter}}&nbsp;</a></td>
    <td>{{task.taskCode}}</td>
    <td>{{task.taskDescription}}</td>
    <td>{{task.processCode}}</td>
    <td>&nbsp;{{task.sequence}}</td>
    <td>&nbsp;{{task.isManual}}</td>
    <td>{{task.dateTimeCreated|date:_("m/d/y H:i:s")}}</td>
    <td>{{task.dateTimeLastModified|date:_("m/d/y H:i:s")}}</td>
    <td>{{task.userEmailCreated}}</td>
    <td>{{task.userEmailLastModified}}</td>
</tr>
{%endfor%}
<!--Add or Remove this submit button as needed <tr>
   <td><input type=submit value="Update"></td>
